{
	"name": "sticky-empty-lines",
	"version": "1.0.0",
	"description": "Obsidian plugin that keeps X empty lines at the bottom while editing and removes them on close.",
	"main": "main.js",
	"scripts": {
		"dev": "esbuild main.ts --bundle --outfile=main.js --watch",
		"build": "esbuild main.ts --bundle --outfile=main.js --minify --external:obsidian --format=cjs"
	},
